<test-suite>
    <test-case>
        <name>timedlab3(2,-1)</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>10000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-value>
                <address>ASKED_VAL</address>
                <value>2</value>
            </test-value>
			<test-array>
                <address>ARRAY</address>
                <value>2,-1</value>
            </test-array>
        </input>
        <output>
             <test-value condition="equals" points="7">
                <address>ASKED_IND</address>
                <value>0</value>
            </test-value>
			<test-value condition="equals" points="7">
                <address>ODD_CNT</address>
                <value>0</value>
            </test-value>
			<test-value condition="equals" points="6">
                <address>LENGTH</address>
                <value>1</value>
            </test-value>
        </output>
    </test-case>
