<?xml version="1.0"?>
<test-suite>    
    <test-case>
        <name>checksum({x5555}) = x5555</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-value><address>LENGTH</address><value>1</value></test-value>
            <test-array><address>ARRAY</address><value>x5555</value></test-array>
        </input>
        <output>
            <test-value points="25"><address>CHECKSUM</address><value>x5555</value></test-value>
        </output>
    </test-case>
